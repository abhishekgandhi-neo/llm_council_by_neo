# OpenRouter API Key
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Council Configuration
# Comma-separated list of model identifiers from OpenRouter
COUNCIL_MODELS=openai/gpt-4o,anthropic/claude-3-sonnet,google/gemini-pro-1.5

# Orchestration Strategy: synthesis, voting
COUNCIL_STRATEGY=synthesis

# Execution Settings
COUNCIL_MAX_RETRIES=3
COUNCIL_TIMEOUT=30.0

# Output Path (for main.py)
COUNCIL_OUTPUT_FILE=council_answer.md